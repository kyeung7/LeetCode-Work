# Write a function to find the longest common prefix string amongst an array of strings.
# If there is no common prefix, return an empty string "".



#CURRENTLY FAILES DOG TEST, PASSES FLIGHT
class Solution(object):
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        selectA = strs[0]
        selectB = 1
        
        print(selectA)
        
        prefixStr = ""
        testStr = ""
        i = 0
        
        print(strs)
        while i < len(strs):
            
            
            for j in range(len(strs[i])):
                try:
                    if strs[i][j] == strs[selectB][j]:
                        print("match: ", strs[i][j])
                        testStr += strs[i][j]
                except IndexError:
                    break
            
            checkIndex = 0
            for k in testStr:
                if prefixStr =="":
                    prefixStr += testStr
                else:
                    try:
                        if k == prefixStr[checkIndex]: #we are looking for longest SHARED prefix
                            continue 
                        else:
                            prefixStr = prefixStr[:checkIndex]
                            pass #delete any characters after index of k from prefixStr
                    except IndexError:
                        continue # same as else, delete non matchs
                checkIndex += 1
                
            selectB +=1
            i+=1
        
        
        return prefixStr
